{
  "name": "vue-boilerplate-ui",
  "main": "vue_boilerplate/output",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "mocha --recursive vue_boilerplate/output/src/test/"
  },
  "directories": {
    "test": "src/test/"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "bootstrap": "^4.5.2",
    "bootstrap-icons-vue": "^0.1.2",
    "bootstrap-vue": "^2.17.0",
    "chart.js": "^2.9.3",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.5.1",
    "popper": "^1.0.1",
    "vue-chartjs": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^4.5.0",
    "markdown-anchor": "^0.2.0",
    "markdown-it": "^11.0.1",
    "markdown-loader": "^5.1.0",
    "mocha": "^8.1.3",
    "vue-template-compiler": "^2.6.11",
    "webpack-chain": "^6.5.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-var": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
